<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="200"
        Width="400" Height="200"
        x:Class="RegexApp.Views.RegexWindow"
        Title="RegexWindow">
  <Grid RowDefinitions="*,*,*">
    <TextBox FontSize="20" Name="inputRegex" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" ></TextBox>
    <Button Name="ok" Grid.Row="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">OK</Button>
    <Button Name="cancel" Grid.Row="2" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">Cancel</Button>
  </Grid>
</Window>
